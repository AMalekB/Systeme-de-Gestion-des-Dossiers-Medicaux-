generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Utilisateur {
  id        Int      @id @default(autoincrement())
  nom       String
  email     String   @unique
  motDePasse String
  role      Role
}

enum Role {
  ADMIN
  MEDECIN
}
